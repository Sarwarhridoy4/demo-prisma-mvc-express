{
  "name": "demo-prisma-express-mvc",
  "private": true,
  "main": "src/server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "migrate": "npx prisma migrate dev",
    "generate": "npx prisma generate",
    "reset": " npx prisma migrate reset",
    "update": " npx prisma db push",
    "studio": "npx prisma studio",
    "postinstall": "prisma generate"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/node": "^25.0.10",
    "@types/pg": "^8.16.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "bcryptjs": "^3.0.3",
    "compression": "^1.8.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.17.2",
    "prisma": "^7.3.0"
  },
  "prisma": {
    "schema": "./prisma"
  }
}
